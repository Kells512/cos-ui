    "cypress:open": "cypress open",
    "changeset": "changeset",
    "release": "changeset publish",
    "version:canary": "changeset version --snapshot canary",
    "release:canary": "changeset publish --tag canary",
    "api:generate": "openapi-merge-cli && openapi-generator-cli generate -i mas-mock/merged-openapi.yaml -g typescript-axios -o packages/api --additional-properties=ngVersion=6.1.7,npmName=@kas-connectors/api,supportsES6=true,withInterfaces=true --skip-validate-spec"
