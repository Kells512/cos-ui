  "scripts": {
    "lerna": "lerna",
    "start": "lerna run start --stream --parallel --scope @kas-connectors/*",
    "test": "lerna run test --",
    "lint": "lerna run lint -- --fix",
    "build": "lerna run build",
    "prepublish": "yarn run api:generate && lerna run prepublish",
    "start:app:example": "yarn run build && yarn --cwd example && yarn --cwd example start",
